			lm36272,default-brightness = <178>;
			lm36272,max-brightness = <255>;
			lm36272,dim-brightness = <1>;
			lm36272,dim-enabled;
			lm36272,blmap-size = <256>;
			lm36272,blmap = <
				0 553 553 553 553 553 553 553 553 553               /* 0-9 */
