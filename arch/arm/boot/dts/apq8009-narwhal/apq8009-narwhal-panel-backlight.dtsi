&soc {
	i2c@78b8000 { /* BLSP1 QUP4 */
		backlight_dsv: lm36272@11 {
			compatible = "leds,lm36272";
			status = "okay";
			reg = <0x11>;
			lm36272,gpio-dsv-p-en = <&msm_gpio 95 0x00>;
