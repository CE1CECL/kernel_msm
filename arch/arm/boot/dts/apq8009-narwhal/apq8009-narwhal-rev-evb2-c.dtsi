#include "apq8009-narwhal.dtsi"

&mdss_dsi0 {
	pinctrl-0 = <&mdss_reset_active &mdss_te_active>;
	pinctrl-1 = <&mdss_reset_suspend &mdss_te_suspend>;

	/delete-property/qcom,platform-enable-gpio;

	vddio-supply = <&pm8916_l6>;
};

&bt_addr {
	allow-random-addr;
};

&i2c_6 {
	status = "ok";
};

&spi_0 {
	status = "disabled";
};

&movement_on_i2c4 {
	status = "disabled";
};

&movement_on_i2c6 {
	status = "ok";
};

&load_sw_control {
	status = "disabled";
};

&sdhc_2 {
	vdd-io-supply = <&pm8916_l12>;
};

&audio_codec_narwhal {
	/delete-property/ qcom,msm-micbias1-ext-cap;
};
