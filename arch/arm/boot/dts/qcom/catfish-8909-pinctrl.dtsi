/* add catfish/catshark pinctrl info*/

&soc {
	uart2 {
                blsp1_uart2_active: blsp1_uart2_active {
                        mux {
                                pins = "gpio20", "gpio21", "gpio111", "gpio112";
                                function = "blsp_uart2";
                        };
                        config {
                                pins = "gpio20", "gpio21", "gpio111", "gpio112";
                                drive-strength = <2>;
                                bias-disable;
                        };
                };
                blsp1_uart2_sleep: blsp1_uart2_sleep {
                        mux {
                                pins = "gpio20", "gpio21", "gpio111", "gpio112";
                                function = "gpio";
                        };
                        config {
                                pins = "gpio20", "gpio21", "gpio111", "gpio112";
                                drive-strength = <2>;
                                bias-pull-down;
                        };
                };
        };

	msm_gpio: pinctrl@1000000 {

		pmx_i2c_6 {
			i2c_6_active: i2c_6_active {
				mux {
					pins = "gpio10", "gpio11";
					function = "blsp_i2c6";
				};
				config {
					pins = "gpio10", "gpio11";
					drive-strength = <2>;
					bias-pull-up;
				};
			};
			i2c_6_sleep: i2c_6_sleep {
				mux {
					pins = "gpio10", "gpio11";
					function = "blsp_i2c6";
				};
				config {
					pins = "gpio10", "gpio11";
					drive-strength = <2>;
					bias-pull-up;
				};
			};
		};

		/* add pingrp for touchscreen */
		pmx_ts_int_active {
			ts_int_active: ts_int_active {
				mux {
					pins = "gpio65";
					function = "gpio";
				};

				config {
					pins = "gpio65";
					drive-strength = <2>;
					bias-pull-up;
				};
			};
		};

		pmx_ts_int_suspend {
			ts_int_suspend: ts_int_suspend {
				mux {
					pins = "gpio65";
					function = "gpio";
				};

				config {
					pins = "gpio65";
					drive-strength = <2>;
					bias-pull-up;
				};
			};
		};

		pmx_ts_reset_active {
			ts_reset_active: ts_reset_active {
				mux {
					pins = "gpio23";
					function = "gpio";
				};

				config {
					pins = "gpio23";
					drive-strength = <2>;
					bias-pull-up;
				};
			};
		};

		pmx_ts_reset_suspend {
			ts_reset_suspend: ts_reset_suspend {
				mux {
					pins = "gpio23";
					function = "gpio";
				};

				config {
					pins = "gpio23";
					drive-strength = <2>;
					bias-pull-up;
				};
			};
		};

		spi4 {
			spi4_default: spi4_default {
				/* active state */
				mux {
					/* MOSI, MISO, CLK */
					pins = "gpio12", "gpio13", "gpio15";
					function = "blsp_spi4";
				};

				config {
					pins = "gpio12", "gpio13", "gpio15";
					drive-strength = <12>; /* 12 MA */
					bias-disable = <0>; /* No PULL */
				};
			};

			spi4_sleep: spi4_sleep {
				/* suspended state */
				mux {
					/* MOSI, MISO, CLK */
					pins = "gpio12", "gpio13", "gpio15";
					function = "blsp_spi4";
				};

				config {
					pins = "gpio12", "gpio13", "gpio15";
					drive-strength = <12>; /* 12 MA */
					bias-disable = <0>; /* No PULL */
				};
			};

			spi4_cs0_active: cs0_active {
				/* CS */
				mux {
					pins = "gpio14";
					function = "blsp_spi4";
				};

				config {
					pins = "gpio14";
					drive-strength = <2>;
					bias-disable = <0>;
				};
			};

			spi4_cs0_sleep: cs0_sleep {
				/* CS */
				mux {
					pins = "gpio14";
					function = "blsp_spi4";
				};

				config {
					pins = "gpio14";
					drive-strength = <2>;
					bias-disable = <0>;
				};
			};
		};

		sensorhub_ctrl {
		/* NRST, BOOT0, AP2SH, SH2AP, SH2APNW */
			sensorhub_ctrl_active: sensorhub_ctrl_active {
				mux {
					pins = "gpio97", "gpio96", "gpio71", "gpio72","gpio70", "gpio73", "gpio8", "gpio9", "gpio99", "gpio110";
					function = "gpio";
				};

				config {
					pins = "gpio97", "gpio96", "gpio71", "gpio72","gpio70", "gpio73", "gpio8", "gpio9", "gpio99", "gpio110";
					drive-strength = <2>;
					bias-disable;
				};
			};
		};

		tlmm_gpio_key {
                        gpio_bt_host_wake_active: gpio_bt_host_wake_active {
                                mux {
                                        pins = "gpio31";
                                        function = "gpio";
                                };
                                config {
                                        pins = "gpio31";
                                        drive-strength = <2>;
                                        bias-pull-down;
                                };
                        };

                        gpio_bt_host_wake_sleep: gpio_bt_host_wake_sleep {
                                mux {
                                        pins = "gpio31";
                                        function = "gpio";
                                };
                                config {
                                        pins = "gpio31";
                                        drive-strength = <2>;
                                        bias-pull-down;
                                };
                        };

			gpio_bt_dev_wake_active: gpio_bt_dev_wake_active {
                                mux {
                                        pins = "gpio32";
                                        function = "gpio";
                                };
                                config {
                                        pins = "gpio32";
                                        drive-strength = <2>;
                                        output-high;
                                        bias-disable;
                                };
                        };

                        gpio_bt_dev_wake_sleep: gpio_bt_dev_wake_sleep {
                                mux {
                                        pins = "gpio32";
                                        function = "gpio";
                                };
                                config {
                                        pins = "gpio32";
                                        drive-strength = <2>;
                                        output-high;
                                        bias-disable;
                                };
                        };

			 gpio_bt_reg_on_active: gpio_bt_reg_on_active {
                                mux {
                                        pins = "gpio34";
                                        function = "gpio";
                                };
                                config {
                                        pins = "gpio34";
                                        drive-strength = <2>;
                                        bias-pull-down;
                                };
                        };

                        gpio_bt_reg_on_sleep: gpio_bt_reg_on_sleep {
                                mux {
                                        pins = "gpio34";
                                        function = "gpio";
                                };
                                config {
                                        pins = "gpio34";
                                        drive-strength = <2>;
                                        bias-pull-down;
                                };
                        };
                };
	};
};
