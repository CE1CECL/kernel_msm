/dts-v1/;

#include "../qcom/apqcobalt-v2.1.dtsi"
#include "../qcom/msmcobalt-camera-sensor-cdp.dtsi"

/ {
	model = "Google, Inc. Polaris P2";
	/* compatible = "goog,polaris", "goog,polaris-p2"; */
	goog,board-revision = "polaris-p2";

	/* TODO(muirj): Remove once the boot-loader can select based
	 * on the above information only. */
	compatible = "qcom,apqcobalt-cdp", "qcom,apqcobalt", "qcom,cdp";
	qcom,board-id = <1 0>;
};

#include "polaris.dtsi"

&headset {
	interrupts = <131 IRQ_TYPE_LEVEL_LOW>;
	pinctrl-0 = <&da7219_interrupt_p2_default>;
};
